{% extends 'base.pebble' %}
      
{% block primary %}
      <div class="row-fluid marketing">
        <div class="span12">
          <h4>{{ content.title }}</h4>
          <p>{{ content.body }}</p>
        </div>

      </div>

      <hr>
      
      <h5>Published Pages</h5>
      {% for page in published_pages %}
      	<a href="{{ config.site_host }}{{ page.uri }}">{{ page.title }}</a>
      {% endfor %}
{% endblock %}
